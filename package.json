{
    "name": "spicetify-css-map-analyzer",
    "description": "Analyzes Spicetify's css-map.json and prints mapped, unmapped classes for current Spotify version",
    "author": "Tetrax-10",
    "license": "MIT",
    "type": "module",
    "main": "src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tetrax-10/spicetify-css-map-analyzer.git"
    },
    "bugs": {
        "url": "https://github.com/Tetrax-10/spicetify-css-map-analyzer/issues"
    },
    "scripts": {
        "format": "prettier --write .",
        "analyze-mapped-css": "node src/index.js --mapped --sort --out",
        "analyze-unmapped-css": "node src/index.js --unmapped --sort --out",
        "analyze-css-map": "node src/index.js --mapped --unmapped --sort --out",
        "extract-remap-data": "node src/index.js --sort --out --extract",
        "remapper": "node src/index.js --remap",
        "analyzer": "node src/index.js"
    },
    "dependencies": {
        "chalk": "^5.2.0",
        "commander": "^10.0.1",
        "diff": "^5.1.0",
        "ini": "^4.1.0",
        "prettier": "^2.8.8"
    }
}
